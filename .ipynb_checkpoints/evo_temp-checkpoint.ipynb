    "def f(psi):\n",
    "    return np.matmul(-1j*H,psi)\n",
    "\n",
    "def RK4(psi):\n",
    "    k1 = f(psi)\n",
    "    k2 = f(psi + k1 * dt / 2.)\n",
    "    k3 = f(psi + k2 * dt / 2.)\n",
    "    k4 = f(psi + k3 * dt)\n",
    "    new_psi = psi + (dt / 6.) * (k1 + 2*k2 + 2*k3 + k4)\n",
    "    return normalize(new_psi,dx)\n",
    "\n",
    "def euler(psi):\n",
    "    new_psi = psi + dt*f(psi)\n",
    "    return normalize(new_psi,dx)\n",
    "    \n",
    "# Initialisation Psi0\n",
    "\n",
